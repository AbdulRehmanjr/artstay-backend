generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  userId      String      @id @default(cuid())
  email       String      @unique
  password    String      @default("none")
  accountType AccountType @default(NONE)
}

enum AccountType {
  NONE
  ARTISAN
  SAFARI
  FAIRS
  BUSINESS
  HOTEL
  ALL
  ADMIN
  SUPERADMIN
}
